DEFAULT_PROG=default_ballot_box_sim
# The stored valid barcode expire at:
# 2019+07+31+22+20
# 2019+07+31+22+22
# 2019+07+31+22+22
DEFAULT_CURRENT_MINUTE=15
$(info DEFAULT_CURRENT_MINUTE=$(DEFAULT_CURRENT_MINUTE))
CFLAGS += -DSIMULATION \
        -DSIMULATION_UART \
        -DHARDCODE_CURRENT_TIME \
        -DCURRENT_YEAR=2019 \
        -DCURRENT_MONTH=7 \
        -DCURRENT_DAY=31 \
        -DCURRENT_HOUR=22 \
        -DCURRENT_MINUTE=$(DEFAULT_CURRENT_MINUTE)
include Makefile.freertos
